<div class="log-in">

<div class="user_welcome">Welcome Back</div>

<hr>
 <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
 <%= devise_error_messages! %>


  <%= f.email_field :email, autofocus: true, placeholder: "Email", autocomplete: "off", class: "form-control" %>
<br>



  <%= f.password_field :password, placeholder: "Password", autocomplete: "off", class: "form-control" %>
<br>


  <%= f.submit "Log In", class: "user_button" %>

<% end %>


</div>
