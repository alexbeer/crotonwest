<head>
  <title>Croton West</title>
</head>
<body>
  <div class="main_div">

    <div id="banner">Exploring, discovering, and photographing North America</div>

    <div class="map">

      <iframe src="<%= asset_path( 'map.html' ) %>" width="900" height="525" style="border: none;"></iframe>
      <script>
              if (window.addEventListener){ window.addEventListener("message", function(event) { if(event.data.length >= 22)
              { if( event.data.substr(0, 22) == "__MM-LOCATION.REDIRECT") location = event.data.substr(22); } }, false); }
              else if (window.attachEvent){ window.attachEvent("message", function(event) { if( event.data.length >= 22)
              { if ( event.data.substr(0, 22) == "__MM-LOCATION.REDIRECT") location = event.data.substr(22); } }, false); } 
      </script>
    </div>
  </div>
</body>
